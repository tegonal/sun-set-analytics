    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "graphql": "^16.10.0",
    "ky": "^1.8.0",
    "next": "15.2.4",
    "payload": "^3.33.0",
    "payload-oapi": "0.2.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "0.34.1",
    "theme-ui": "^0.17.2"
