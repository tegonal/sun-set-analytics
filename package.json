    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "graphql": "^16.8.1",
    "ky": "^1.8.0",
    "next": "15.2.4",
    "payload": "^3.31.0",
    "payload-oapi": "^0.2.0-b.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "0.32.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.2.4",
    "prettier": "^3.4.2",
    "typescript": "5.8.2"
